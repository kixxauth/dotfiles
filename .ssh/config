# Multiple Connections
ControlMaster auto
ControlPath /tmp/ssh_mux_%h_%p_%r

# Remote server
Host kristo.us
  HostName kristo.us
  User kris
  IdentityFile ~/.ssh/id_kris_rsa
  Port 2575

# Default GitHub user (kixxauth)
Host github.com
  HostName github.com
  User git
  IdentityFile ~/.ssh/id_rsa
 
# Client user (FireworksProject)
Host github-fireworksproject
  HostName github.com
  User git
  IdentityFile ~/.ssh/id_rsa_fireworksproject
