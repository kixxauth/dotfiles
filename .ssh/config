# Multiple Connections
ControlMaster auto
ControlPath /tmp/ssh_mux_%h_%p_%r

# Default GitHub user (kixxauth)
Host github.com
  HostName github.com
  User git
  IdentityFile ~/.ssh/id_rsa
 
# Client user (FireworksProject)
Host github-fireworksproject
  HostName github.com
  User git
  IdentityFile ~/.ssh/id_rsa_fireworksproject
